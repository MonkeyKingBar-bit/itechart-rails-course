<%if obj.any?%>
  <br />
  <div class="container">
    <table class="table table-striped table-bordered table-hover">
      <thead class="thead-dark">
      <tr>
        <th> Category </th>
        <th> Amount </th>
      </tr>
      </thead>

      <tbody>
      <% obj.each do |category|%>
        <tr>
          <td><%= category[0] %></td>
          <td><%= category[1] %></td>
        </tr>
      <% end%>
      </tbody>
    </table>
  </div>
<% end %>